visitedNodesInOrder, calculatePath(startNode)];\r\n}\r\n\r\nfunction calculatePath(finishNode) {\r\n  const shortestPathNodes = [];\r\n  let currentNode = finishNode;\r\n  console.log(\"the unshifting\");\r\n  while (currentNode !== null) {\r\n\r\n      shortestPathNodes.unshift(currentNode);\r\n      currentNode = currentNode.previousNode;\r\n\r\n      console.log(shortestPathNodes);\r\n  }\r\n  return shortestPathNodes;\r\n}\r\n\r\nfunction getAllNeighbors(grid, node, size) {\r\n\r\n  const neighbors = [];\r\n  const point2 = node.point2;\r\n   \r\n  for(let i=0; i <size; i++){\r\n    var tempNode = grid[point2][i]; \r\n    if((!(tempNode.isVisited && grid[i][point2].isVisited)) && tempNode.distance > 0){\r\n        neighbors.push(tempNode);\r\n        for(let j=0; j <size; j++){\r\n          grid[i][j].isVisited = true;\r\n        }\r\n    }\r\n    \r\n  }\r\n\r\n   return neighbors;\r\n}\r\n"],"mappings":"AAAA,OAAO,SAASA,GAAG,CAACC,IAAI,EAAEC,SAAS,EAAEC,UAAU,EAAEC,IAAI,EAAE;EACrD,MAAMC,KAAK,GAAG,EAAE;EAChB,MAAMC,mBAAmB,GAAG,EAAE;EAC9BC,OAAO,CAACC,GAAG,CAAC,UAAU,CAAC;EAGvBN,SAAS,CAACO,SAAS,GAAG,IAAI;EAC1BH,mBAAmB,CAACI,IAAI,CAACR,SAAS,CAAC;EAEnCG,KAAK,CAACK,IAAI,CAACR,SAAS,CAAC;EAErB,OAAOG,KAAK,CAACM,MAAM,GAAC,CAAC,EAAE;IACnB,IAAIC,IAAI,GAAGP,KAAK,CAACQ,KAAK,EAAE;IAExB,IAAID,IAAI,CAACE,MAAM,KAAKX,UAAU,EAC1B,OAAO,CAACG,mBAAmB,EAAES,aAAa,CAACH,IAAI,CAAC,CAAC;IAGrD,MAAMI,SAAS,GAAGC,eAAe,CAAChB,IAAI,EAAEW,IAAI,EAAER,IAAI,CAAC;IACnDG,OAAO,CAACC,GAAG,CAAC,eAAe,CAAC;IAC5BD,OAAO,CAACC,GAAG,CAACQ,SAAS,CAAC;IAEtB,KAAK,MAAME,QAAQ,IAAIF,SAAS,EAAE;MAC9BE,QAAQ,CAACT,SAAS,GAAG,IAAI;MACzBS,QAAQ,CAACC,YAAY,GAAGP,IAAI;MAC5BN,mBAAmB,CAACI,IAAI,CAACQ,QAAQ,CAAC;MAClCb,KAAK,CAACK,IAAI,CAACQ,QAAQ,CAAC;IAExB;EACJ;EAEA,OAAO,CAACZ,mBAAmB,EAAES,aAAa,CAACb,SAAS,CAAC,CAAC;AACxD;AAEA,SAASa,aAAa,CAACZ,UAAU,EAAE;EACjC,MAAMiB,iBAAiB,GAAG,EAAE;EAC5B,IAAIC,WAAW,GAAGlB,UAAU;EAC5BI,OAAO,CAACC,GAAG,CAAC,gBAAgB,CAAC;EAC7B,OAAOa,WAAW,KAAK,IAAI,EAAE;IAEzBD,iBAAiB,CAACE,OAAO,CAACD,WAAW,CAAC;IACtCA,WAAW,G